{
  "name": "react-static-boilerplate",
  "version": "2.0.0",
  "description": "A boilerplate for building static sites with React and React Router",
  "repository": {
    "type": "git",
    "url": "https://github.com/iansinnott/react-static-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/iansinnott/react-static-boilerplate/issues"
  },
  "author": "Ian Sinnott <ian@iansinnott.com> (http://iansinnott.com)",
  "license": "MIT",
  "homepage": "",
  "engines": {
    "node": "^6.9.1"
  },
  "devDependencies": {
    "app-time": "^0.7.2",
    "babel-eslint": "^7.1.1",
    "cross-env": "^3.1.3",
    "eslint": "^3.11.1",
    "eslint-config-zen": "^2.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-react": "^6.8.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "d3": "^4.4.0",
    "d3-ease": "^1.0.2",
    "debug": "2.5.2",
    "flow-bin": "^0.37.4",
    "font-awesome": "^4.7.0",
    "history": "^4.5.0",
    "immutable": "^3.8.1",
    "immutable-ext": "^1.0.8",
    "normalize.css": "^5.0.0",
    "ramda": "^0.23.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^3.0.0",
    "rxjs": "^5.0.2"
  },
  "apptime": {
    "vendorLibs": [
      "classnames",
      "d3",
      "d3-ease",
      "debug",
      "flow-bin",
      "history",
      "immutable",
      "immutable-ext",
      "normalize.css",
      "ramda",
      "react",
      "react-dom",
      "react-router",
      "rxjs"
    ]
  },
  "scripts": {
    "start": "app-time start",
    "postinstall": "app-time setup",
    "prebuild": "rimraf build",
    "build": "app-time build",
    "lint": "eslint client",
    "conf": "node ./scripts/generate-nginx-conf.js",
    "test": "echo 'No tests specified.'",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "bump:patch": "npm version patch -m \"v%s\"",
    "bump:minor": "npm version minor -m \"v%s\"",
    "bump:major": "npm version major -m \"v%s\"",
    "bump": "npm run bump:patch"
  }
}
