[ignore]
.*/node_modules/fbjs/.*
.*/node_modules/flow-runtime/.*

[include]

[libs]
flow-typed

[options]
module.name_mapper.extension='styl' -> '<PROJECT_ROOT>/CSSModule.js.flow'
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/CSSModule.js.flow'
module.name_mapper.extension='png' -> '<PROJECT_ROOT>/AssetModule.js.flow'
module.name_mapper.extension='svg' -> '<PROJECT_ROOT>/AssetModule.js.flow'
module.name_mapper.extension='ico' -> '<PROJECT_ROOT>/AssetModule.js.flow'
module.name_mapper.extension='jpg' -> '<PROJECT_ROOT>/AssetModule.js.flow'
module.name_mapper.extension='md' -> '<PROJECT_ROOT>/AssetModule.js.flow'
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=web_modules
module.use_strict=true
esproposal.export_star_as=enable
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
